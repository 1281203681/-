page {
  background-color: #f0f0f0;
}
.all_wrap .content_wrap {
  padding-top: 12rpx;
  margin-bottom: 24rpx;
  display: flex;
  flex-direction: column;
  background-color: white;
}
.all_wrap .content_wrap .title_wrap {
  display: flex;
}
.all_wrap .content_wrap .title_wrap image {
  width: 100rpx;
  height: 100rpx;
  border-radius: 50%;
}
.all_wrap .content_wrap .title_wrap .info_wrap .user_name {
  font-weight: 600;
  color: #81e5e9;
}
.all_wrap .content_wrap .title_wrap .info_wrap .user_info {
  padding: 6rpx;
  font-size: 32rpx;
  background-color: #fa9d9d;
  color: white;
}
.all_wrap .content_wrap .mess_wrap {
  display: flex;
  flex-direction: column;
  padding-top: 12rpx;
}
.all_wrap .content_wrap .mess_wrap .tip {
  width: 96rpx;
  text-align: center;
  color: white;
  padding: 6rpx;
  border-radius: 12rpx;
  font-size: 32rpx;
  background-color: #f37f92;
  margin-left: 12rpx;
}
.all_wrap .content_wrap .mess_wrap .mess {
  padding: 12rpx;
  max-width: max-content;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
}
.all_wrap .content_wrap .mess_wrap .img {
  text-align: center;
}
.all_wrap .content_wrap .mess_wrap .sendTime {
  font-size: 28rpx;
  padding: 12rpx;
  font-weight: 100;
  color: #949494;
}
.all_wrap .content_wrap .mess_wrap .sendTime .del_tip {
  float: right;
  margin-right: 24rpx;
  color: #1884dd;
}
