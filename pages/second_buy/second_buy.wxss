page {
  background-color: #f5f5f5;
}
.all_wrap .item_wrap {
  display: flex;
  flex-direction: row;
  padding: 12rpx;
  background-color: white;
  margin-bottom: 12rpx;
}
.all_wrap .item_wrap .img_wrap {
  display: flex;
  flex-direction: column;
}
.all_wrap .item_wrap .img_wrap .goods_img {
  width: 180rpx;
  height: 180rpx;
}
.all_wrap .item_wrap .img_wrap .user_wrap {
  display: flex;
  flex-direction: row;
  padding-top: 12rpx;
}
.all_wrap .item_wrap .img_wrap .user_wrap .user_img {
  width: 40rpx;
  height: 40rpx;
  border-radius: 50%;
}
.all_wrap .item_wrap .img_wrap .user_wrap .user_name {
  font-size: 18rpx;
  text-align: center;
  max-width: 80rpx;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
}
.all_wrap .item_wrap .contene_wrap {
  display: flex;
  flex-direction: column;
  justify-content: space-around;
}
.all_wrap .item_wrap .contene_wrap .content {
  padding: 24rpx;
  max-width: 60%;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
}
.all_wrap .item_wrap .contene_wrap .price {
  color: red;
  margin-top: 12rpx;
}
.tip {
  text-align: center;
}
